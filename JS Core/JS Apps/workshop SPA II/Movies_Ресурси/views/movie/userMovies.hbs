{{>header}}
    <div id="myMovies">
        <h1>My Movies</h1>
        <div class="movies">
            {{#each myMovies}}
            <div class="movie">
                <h2>{{ title }}</h2>
                <img src="{{imageUrl}}">
                <p>Available Tickets: {{tickets}}</p>
                <div class="btn-group">
                    <button><a href="#/editMovie/{{_id}}">Edit</a></button>
                    <button><a href="#/deleteMovie/{{_id}}">Delete</a></button>
                    <button><a href="#">Buy Ticket</a></button>
                    <button><a href="#/detailsEvent/{{_id}}">Details</a></button>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
{{>footer}}